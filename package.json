{
  "name": "@pnpm/logger",
  "version": "4.0.0",
  "description": "Logger for pnpm",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
    "test": "tslint -c tslint.json --project . && preview && ts-node test --type-check",
    "tsc": "rimraf lib && tsc",
    "prepublishOnly": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/logger.git"
  },
  "engines": {
    "node": ">=12.17"
  },
  "keywords": [
    "pnpm",
    "logger"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "z@kochan.io",
    "url": "https://www.kochan.io/",
    "twitter": "ZoltanKochan"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/logger/issues"
  },
  "homepage": "https://github.com/pnpm/logger#readme",
  "dependencies": {
    "bole": "^5.0.0",
    "ndjson": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.5",
    "@types/tape": "^4.13.2",
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "package-preview": "^4.0.0",
    "rimraf": "^3.0.2",
    "tape": "^5.6.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  }
}
